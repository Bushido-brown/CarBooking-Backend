    "/api/v1/auth/sign_up": {
      "post": {
        "summary": "create registration",
        "tags": [
          "SIGNUP"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "sign_up",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "name",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "user created",
            "headers": {
              "Authorization": {
                "schema": {
                  "type": "string",
                  "nullable": true
                },
                "description": "Bearer Token that we use to login the user "
              }
            },
            "content": {
              "application/json": {
                "examples": {
                  "successfull_signup": {
                    "value": {
                      "status": "Created",
                      "message": "created users",
                      "data": {
                        "id": 1,
                        "name": "jon doe",
                        "created_at": "2022-10-01T23:14:34.994Z",
                        "updated_at": "2022-10-01T23:14:34.994Z"
                      },
                      "header": {
                        "Authorization": "Bearer eyJhbGciOiJIUzUxMiJ9.eyJ1c2VyX2lkIjoxMiwidG9rZW4iOiJ4dUFIck43ZGV6N3k1WlBSTXEzb0F1MW5mYVA5ODVLRyIsImV4cCI6MTY2OTY4MTI0NH0.D3J3CnrrX5K4WJMdVmtP5qjWcIHUIqQBAWsGWhi6hy7yZrUju8tzIslhdrXTC5tncOWEA8gADG89NqNeZxU5lg"
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "description": "failed user creat action ",
            "content": {
              "application/json": {
                "examples": {
                  "blank_password": {
                    "value": {
                      "status": "422 Unprocessable Entity",
                      "message": "Error processing data",
                      "data": {
                        "errors": [
                          "Password can't be blank"
                        ]
                      }
                    }
                  },
                  "blank_name": {
                    "value": {
                      "status": "422 Unprocessable Entity",
                      "message": "Error processing data",
                      "data": {
                        "errors": [
                          "Name can't be blank"
                        ]
                      }
                    }
                  },
                  "blanK_body": {
                    "value": {
                      "status": "422 Unprocessable Entity",
                      "message": "Error processing data",
                      "data": {
                        "errors": [
                          "Password can't be blank",
                          "Name can't be blank"
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/sign_in": {
      "post": {
        "summary": "create session",
        "tags": [
          "Login"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "login",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "name",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful",
            "headers": {
              "Authorization": {
                "schema": {
                  "type": "string",
                  "nullable": true
                },
                "description": "Bearer Token that we will use in other consecutive requests"
              }
            },
            "content": {
              "application/json": {
                "examples": {
                  "successfull_login": {
                    "value": {
                      "data": {
                        "token": "Bearer eyJhbGciOiJIUzUxMiJ9.eyJ1c2VyX2lkIjozLCJ0b2tlbiI6Ino1VDZQTWdUY2Y3TERtRmlwSG5TM3pTUFphU3dRWHlFIiwiZXhwIjoxNjY5NjgzMzc2fQ.8E9DtWViDxWW_QG7YHqb76_W69Ew02ovTdL26YM2DBmkdw8Px0amiGFrRJnqcZSiKAYJuAVsn43JmOlmWtaPRQ"
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "description": "Unprocessable Entity",
            "content": {
              "application/json": {
                "examples": {
                  "invalid_username_or_password": {
                    "value": {
                      "errors": [
                        "translation missing: en.errors.controllers.auth.invalid_credentials"
                      ]
                    }
                  },
                  "missing_username_or_password": {
                    "value": {
                      "errors": [
                        "translation missing: en.errors.controllers.insufficient_params"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/sign_out": {
      "delete": {
        "summary": "delete session",
        "tags": [
          "Log out"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "logout",
            "in": "headers",
            "schema": {
              "type": "object",
              "properties": {
                "Authorization": {
                  "type": "string"
                }
              },
              "required": [
                "Authorization"
              ]
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content",
            "content": {
              "application/json": {
                "examples": {
                  "successfull_logout": {
                    "value": {
                      "data": {
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized ",
            "content": {
              "application/json": {
                "examples": {
                  "unauthorized_logout_request": {
                    "value": {
                      "errors": [
                        "translation missing: en.errors.controllers.auth.unauthenticated"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
