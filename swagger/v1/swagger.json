    "/api/v1/auth/sign_in": {
      "post": {
        "summary": "create session",
        "tags": [
          "session"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "login",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "successful",
            "headers": {
              "Authorization": {
                "schema": {
                  "type": "string",
                  "nullable": true
                },
                "description": "Bearer Token that we will use in other consecutive requests"
              }
            },
            "content": {
              "application/json": {
                "examples": {
                  "successfull_login": {
                    "value": {
                      "data": {
                        "token": "Bearer **"
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "description": "Unprocessable Entity",
            "content": {
              "application/json": {
                "examples": {
                  "invalid_username_or_password": {
                    "value": {
                      "errors": [
                        "translation missing: en.errors.controllers.auth.invalid_credentials"
                      ]
                    }
                  },
                  "missing_username_or_password": {
                    "value": {
                      "errors": [
                        "translation missing: en.errors.controllers.insufficient_params"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
